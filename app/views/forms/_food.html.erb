<%= form_for @foodexp do |f| %>

<h3>Monthly</h3>

<div class="form-group">
  <div class="field">
    <div class="newfood">

        <%= f.label :monthlyfood, "Monthly Grocery Costs" %>
        <%= f.text_field :monthlyfood %>

    </div>
  </div>
</div>

<%= f.submit "Add/Update Food Expenses", class: "btn btn-primary form-control" %>

<% end %>
